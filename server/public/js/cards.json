[    
    {
        "type": "text",
        "description": "Posé là, simplement.",
        "overtitle": "La simplicité",
        "subtitle": "Fragment 0",
        "shortcut": "0"
    },
    {
        "type": "text",
        "description": "Admirant tous les jours le même décor.",
        "overtitle": "L’admiration",
        "subtitle": "Fragment 1",
        "shortcut": "1"
    },

    {
        "type": "text",
        "description": "Les passants, les habitants, les invités.",
        "overtitle": "Le monde",
        "subtitle": "Fragment 2",
        "shortcut": "2"
    },

    {
        "type": "text",
        "description": "Tous, se bousculent et passent à côté.",
        "overtitle": "Le passage",
        "subtitle": "Fragment 3",
        "shortcut": "3"
    },

    {
        "type": "text",
        "description": "Posé là, depuis je ne sais plus trop quand.",
        "overtitle": "Le savoir",
        "subtitle": "Fragment 4",
        "shortcut": "4"
    },

    {
        "type": "text",
        "description": "Ramené puis déposé.",
        "overtitle": "La mène",
        "subtitle": "Fragment 5",
        "shortcut": "5"
    },

    {
        "type": "text",
        "description": "Mon rôle, si bref, déjà terminé.",
        "overtitle": "Le rôle",
        "subtitle": "Fragment 6",
        "shortcut": "6"
    },

    {
        "type": "text",
        "description": "Un souvenir de voyage, rapidement aimé.",
        "overtitle": "Le souvenir",
        "subtitle": "Fragment 7",
        "shortcut": "7"
    },

    {
        "type": "text",
        "description": "Puis oublié.",
        "overtitle": "L’oubli",
        "subtitle": "Fragment 8",
        "shortcut": "8"
    },

    {
        "type": "text",
        "description": "Céramique ou plastique.",
        "overtitle": "La matière",
        "subtitle": "Fragment 9",
        "shortcut": "9"
    },

    {
        "type": "text",
        "description": "À la main ou d’usine.",
        "overtitle": "La fabrication",
        "subtitle": "Fragment 10",
        "shortcut": "10"
    },

    {
        "type": "text",
        "description": "Un paysage, un drapeau ou une blague.",
        "overtitle": "L’identité",
        "subtitle": "Fragment 11",
        "shortcut": "11"
    },

    {
        "type": "text",
        "description": "Qu’importe ma forme, ma couleur.",
        "overtitle": "La forme",
        "subtitle": "Fragment 12",
        "shortcut": "12"
    },

    {
        "type": "text",
        "description": "Ma présence pourtant permanente.",
        "overtitle": "La présence",
        "subtitle": "Fragment 13",
        "shortcut": "13"
    },

    {
        "type": "text",
        "description": "Aussi éphémère que votre bonheur.",
        "overtitle": "Le bonheur",
        "subtitle": "Fragment 14",
        "shortcut": "14"
    },

    {
        "type": "text",
        "description": "Souvenir  d’un lieu, d’un voyage, d’une époque.",
        "overtitle": "Le souvenir",
        "subtitle": "Fragment 15",
        "shortcut": "15"
    },

    {
        "type": "text",
        "description": "Posé là, indéfiniment, sur le frigo des parents.",
        "overtitle": "L’infini",
        "subtitle": "Fragment 16",
        "shortcut": "16"
    },

    {
        "type": "text",
        "description": "Attendant d’être rejoint par d’autres.",
        "overtitle": "L’attente",
        "subtitle": "Fragment 17",
        "shortcut": "17"
    },

    {
        "type": "text",
        "description": "Souvenirs d’aventures que je ne vivrai pas.",
        "overtitle": "La vie",
        "subtitle": "Fragment 18",
        "shortcut": "18"
    },

    {
        "type": "text",
        "description": "Attendant d’être remplacé, par un, plus récent que moi.",
        "overtitle": "La relève",
        "subtitle": "Fragment 19",
        "shortcut": "19"
    },

    {
        "type": "text",
        "description": "Agrippé depuis toujours.",
        "overtitle": "L’attache",
        "subtitle": "Fragment 20",
        "shortcut": "20"
    },

    {
        "type": "text",
        "description": "Au service des uns, des autres.",
        "overtitle": "L’autre",
        "subtitle": "Fragment 21",
        "shortcut": "21"
    },

    {
        "type": "text",
        "description": "Sans un merci, jamais.",
        "overtitle": "Le remerciement",
        "subtitle": "Fragment 22",
        "shortcut": "22"
    },

    {
        "type": "text",
        "description": "Mais des regards, ça oui.",
        "overtitle": "Le regard",
        "subtitle": "Fragment 23",
        "shortcut": "23"
    },

    {
        "type": "text",
        "description": "Le reflet des autres.",
        "overtitle": "Le reflet",
        "subtitle": "Fragment 24",
        "shortcut": "24"
    },

    {
        "type": "text",
        "description": "Dans le meilleur et le pire jour.",
        "overtitle": "Le jour",
        "subtitle": "Fragment 25",
        "shortcut": "25"
    },


    {
        "type": "text",
        "description": "Sous la lumière et dans le noir.",
        "overtitle": "La lumière",
        "subtitle": "Fragment 26",
        "shortcut": "26"
    },

    {
        "type": "text",
        "description": "Peut-être le seul à les connaître vraiment.",
        "overtitle": "La connaissance",
        "subtitle": "Fragment 27",
        "shortcut": "27"
    },

    {
        "type": "text",
        "description": "Espion en un sens.",
        "overtitle": "Le sens",
        "subtitle": "Fragment 28",
        "shortcut": "28"
    },

    {
        "type": "text",
        "description": "Mais activité forcée.",
        "overtitle": "La contrainte",
        "subtitle": "Fragment 29",
        "shortcut": "29"
    },

    {
        "type": "text",
        "description": "Usage unique.",
        "overtitle": "L’unicité",
        "subtitle": "Fragment 30",
        "shortcut": "30"
    },

    {
        "type": "text",
        "description": "Aucune liberté.",
        "overtitle": "La liberté",
        "subtitle": "Fragment 31",
        "shortcut": "31"
    },

    {
        "type": "text",
        "description": "Le poids pèse plus.",
        "overtitle": "Le poids",
        "subtitle": "Fragment 32",
        "shortcut": "32"
    },

    {
        "type": "text",
        "description": "Avec le temps.",
        "overtitle": "Le temps",
        "subtitle": "Fragment 33",
        "shortcut": "33"
    },

    {
        "type": "text",
        "description": "S’amasse la poussière, celle de chacun.",
        "overtitle": "La poussière",
        "subtitle": "Fragment 34",
        "shortcut": "34"
    },

    {
        "type": "text",
        "description": "Parents, enfants, et même ceux d'avant.",
        "overtitle": "La famille",
        "subtitle": "Fragment 35",
        "shortcut": "35"
    },

    {
        "type": "text",
        "description": "J’ai vu défiler des générations entières.",
        "overtitle": "La génération",
        "subtitle": "Fragment 36",
        "shortcut": "36"
    },

    {
        "type": "text",
        "description": "Un peu fatigué, rouillé sur les extrémités.",
        "overtitle": "La fatigue",
        "subtitle": "Fragment 37",
        "shortcut": "37"
    },

    {
        "type": "text",
        "description": "Peut-être que le prochain saura me rafistoler.",
        "overtitle": "La réparation",
        "subtitle": "Fragment 38",
        "shortcut": "38"
    },

    {
        "type": "text",
        "description": "J’en entends, des paroles.",
        "overtitle": "L’ouïe",
        "subtitle": "Fragment 39",
        "shortcut": "39"
    },

    {
        "type": "text",
        "description": "J’assiste aux discours qui ne naîtront jamais.",
        "overtitle": "La vue",
        "subtitle": "Fragment 40",
        "shortcut": "40"
    },

    {
        "type": "text",
        "description": "Aux concerts qui ne seront jamais programmés.",
        "overtitle": "Le rêve",
        "subtitle": "Fragment 41",
        "shortcut": "41"
    },

    {
        "type": "text",
        "description": "Aux scènes d’amour endiablées.",
        "overtitle": "L’amour",
        "subtitle": "Fragment 42",
        "shortcut": "42"
    },

    {
        "type": "text",
        "description": "Un voyeur qui ne peut faire autrement.",
        "overtitle": "Le voyeur",
        "subtitle": "Fragment 43",
        "shortcut": "43"
    },

    {
        "type": "text",
        "description": "Montrer le reflet de ce qu’il se passe réellement.",
        "overtitle": "Le reflet",
        "subtitle": "Fragment 44",
        "shortcut": "44"
    },

    {
        "type": "text",
        "description": "Laisser ensuite libre cours à l’interprétation.",
        "overtitle": "Le libre-arbitre",
        "subtitle": "Fragment 45",
        "shortcut": "45"
    },

    {
        "type": "text",
        "description": "Des images et des sensations.",
        "overtitle": "La sensation",
        "subtitle": "Fragment 46",
        "shortcut": "46"
    },

    {
        "type": "text",
        "description": "Suspendu comme flottant.",
        "overtitle": "La suspension",
        "subtitle": "Fragment 47",
        "shortcut": "47"
    },

    {
        "type": "text",
        "description": "Une vue imprenable sur tout ce qui se déroule.",
        "overtitle": "L’observation",
        "subtitle": "Fragment 48",
        "shortcut": "48"
    },

    {
        "type": "text",
        "description": "Sous le regard inattentif.",
        "overtitle": "L’inattention",
        "subtitle": "Fragment 49",
        "shortcut": "49"
    },

    {
        "type": "text",
        "description": "Des passants.",
        "overtitle":"Le passant",
        "subtitle": "Fragment 50",
        "shortcut": "50"
    },

    {
        "type": "text",
        "description": "Des habitants.",
        "overtitle":"L’habitant",
        "subtitle": "Fragment 51",
        "shortcut": "51"
    },

    {
        "type": "text",
        "description": "J’ensoleille leur vie.",
        "overtitle":"La lumière",
        "subtitle": "Fragment 52",
        "shortcut": "52"
    },

    {
        "type": "text",
        "description": "Je crois.",
        "overtitle":"La croyance",
        "subtitle": "Fragment 53",
        "shortcut": "53"
    },

    {
        "type": "text",
        "description": "J’ensoleille leur soirée.",
        "overtitle":"L’obscurité",
        "subtitle": "Fragment 54",
        "shortcut": "54"
    },

    {
        "type": "text",
        "description": "C’est certain.",
        "overtitle":"La certitude",
        "subtitle": "Fragment 55",
        "shortcut": "55"
    },

    {
        "type": "text",
        "description": "Pour moi, pas de remerciements.",
        "overtitle":"La gratitude",
        "subtitle": "Fragment 56",
        "shortcut": "56"
    },

    {
        "type": "text",
        "description": "Un regard parfois mais très rarement.",
        "overtitle":"Le regard",
        "subtitle": "Fragment 57",
        "shortcut": "57"
    },

    {
        "type": "text",
        "description": "Un coup de chiffon, me manque cruellement.",
        "overtitle":"Le manque",
        "subtitle": "Fragment 58",
        "shortcut": "58"
    },

    {
        "type": "text",
        "description": "Brillant dans un sens.",
